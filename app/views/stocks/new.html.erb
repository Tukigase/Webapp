<h1>新規投稿</h1>
<%= render 'error_messages', locals: { stock: @stock } %>
<%= form_with url: stocks_path, method: :create, local: true do |f| %>
    <h4>商品名</h4>
    <%= f.text_field :name %>

    <h4>在庫数</h4>
    <%= f.number_field :num , min: 0%>

    <h4>詳細</h4>
    <%= f.text_area :detail, size: "60x10" %>

    <h4>画像</h4>
    <%= f.file_field :image %>
    <br>
    <%= f.submit '追加' , class: "button_submit"%>
<% end %>
